<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  <servlet-name>AddUser</servlet-name>
  <servlet-class>com.user.management.system.AddUserServlet</servlet-class>
  </servlet>
  <servlet>
  <servlet-name>DisplayUsers</servlet-name>
  <servlet-class>com.user.management.system.DisplayAllUserServlet</servlet-class>
  </servlet>
  <servlet>
  <servlet-name>GetUpdateUser</servlet-name>
  <servlet-class>com.user.management.system.GetUpdateUserDetailsServlet</servlet-class>
  </servlet>
  <servlet>
  <servlet-name>Updated</servlet-name>
  <servlet-class>com.user.management.system.UpdateUserServlet</servlet-class>
  </servlet>
  <servlet>
  <servlet-name>DeleteUser</servlet-name>
  <servlet-class>com.user.management.system.DeleteUserServlet</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>AddUser</servlet-name>
  <url-pattern>/addUser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  <servlet-name>DisplayUsers</servlet-name>
  <url-pattern>/displayAllUser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  <servlet-name>GetUpdateUser</servlet-name>
  <url-pattern>/updateUser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  <servlet-name>Updated</servlet-name>
  <url-pattern>/updated</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  <servlet-name>DeleteUser</servlet-name>
  <url-pattern>/deleteUser</url-pattern>
  </servlet-mapping>
  
  
</web-app>
